{
    "name": "maikuro/distributed-configuration-bundle",
    "description": "Distributed configuration based on a key value store library",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "eljam",
            "email": "guillaume.cavana@gmail.com"
        }
    ],
    "minimum-stability": "beta",
    "require": {
        "php": "^5.4|^7.0",
        "friendsofsymfony/rest-bundle": "^1.7",
        "jms/serializer-bundle": "~0.11|~1.0",
        "symfony/framework-bundle": "~2.3|~3.0",
        "symfony/dependency-injection": "~2.3|~3.0",
        "symfony/routing": "~2.3|~3.0",
        "symfony/yaml": "~2.3|~3.0",
        "symfony/form": "~2.3|~3.0",
        "symfony/validator": "~2.3|~3.0",
        "symfony/config": "~2.3|~3.0",
        "webmozart/key-value-store": "1.0.0-beta7"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.4",
        "symfony/phpunit-bridge": "^2.7|^3.0",
        "mmoreram/php-formatter": "^1.1",
        "satooshi/php-coveralls": "^1.0",
        "predis/predis": "^1.0",
        "doctrine/dbal" : "^2.4",
        "webmozart/json": "^1.1.1",
        "symfony/browser-kit": "~2.3|~3.0",
        "doctrine/doctrine-cache-bundle": "^1.3"
    },
    "autoload": {
        "psr-4": { "Maikuro\\DistributedConfigurationBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true
}
